<Page x:Class="Esoft.Pages.ThePropertyPages.AddThePropertyPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Esoft.Pages.ThePropertyPages"
      mc:Ignorable="d" 
      d:DesignHeight="365" d:DesignWidth="625"
      Title="AddThePropertyPage">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Grid.ColumnSpan="2" Grid.Row="0">
            <Button Name="BtnHouse" Content="Дом" Click="BtnHouse_Click"/>
            <Button Name="BtnApart" Content="Квартира" Click="BtnApart_Click"/>
            <Button Name="BtnPlot" Content="Земля" Click="BtnPlot_Click"/>
        </StackPanel>

        <Frame Name="objectFrame" Grid.Row="1" Grid.ColumnSpan="2"/>
    </Grid>
</Page>
